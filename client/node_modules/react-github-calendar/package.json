{
  "name": "react-github-calendar",
  "version": "3.1.2",
  "description": " A flexible React component to display a GitHub contributions graph",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "homepage": "https://grubersjoe.github.io/react-github-calendar/",
  "repository": "grubersjoe/react-github-calendar",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write './*' 'src/**/*' 'example/**/*'",
    "lint": "eslint 'src/**/*' 'example/**/*'",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn run build",
    "prepublishOnly": "npx tsc && npm run lint && npm run build",
    "start": "rollup -c -w"
  },
  "dependencies": {
    "react-activity-calendar": "^1.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-tooltip": "^4.2.0"
  },
  "files": [
    "dist"
  ],
